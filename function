Function loopClass()
Dim i As Integer

For i = 1 To 10
    'Cells(i, 1).Value = 100
    Cells(i, 1).Value = "hello"
Next i

End Function



Function printTable()
Dim i As Integer
Dim x As Integer

For i = 1 To 10
 
 'MsgBox (i * a)
x = i * 2
Cells(i, 2).Value = i & "*" & 2 & "=" & x

Next i

End Function
